<jde-content>

    <bullet name="arc" >
    
    	<sound name="bullet_arc" file="res/audio/touhou/sfx/se_tan00.wav" gain="0.25" />

        <hit-body-ref ref="touhou_bullet_v_shape" />

        <animation name="arc_ref_halfsize">
            <frame>
                <sprite-ref ref="touhou_bullet_v_shape_red" />
                <duration>1000</duration>
            </frame>
        </animation>

        <movement-ref ref="arc" />
    </bullet>

    <bullet name="stamp" >
    
    	<sound name="bullet_stamp" file="res/audio/touhou/sfx/se_tan01.wav" gain="0.5" />

        <hit-body-ref ref="touhou_bullet_seal" />

        <animation name="stamp_red_halfsize">
			<frame>
                <sprite-ref ref="touhou_bullet_seal_red" />
                <duration>1000</duration>
            </frame>
        </animation>

        <movement-ref ref="random" />
    </bullet>

    <bullet name="butterfly_darkblue" >
    
